set info size:time
set ratios 1
set scrolloff 10
map a push %mkdir<space>
map e $vim "$f"
map o &play -q "$f"
map P !play "$f"
map p : paste; clear
map c clear
%mkdir -p ~/.cache/.trash
cmd trash &set -f; mv -t ~/.cache/.trash $fx "$fx"
map T trash
map gl cd ~/.dotfiles/.config/lf
map gn cd ~/.dotfiles/.config/newsboat
map gN cd ~/.config/newsboat
map gs cd ~/.dotfiles/.config/suckless
map gv cd ~/.dotfiles/.config/vim
map gy cd ~/.dotfiles/.config/yt-dlp
map gx cd ~/.dotfiles/.config/x11
map gD cd ~/.dotfiles
map gc cd ~/.config
map gC cd ~/.cache
map gt cd ~/.cache/.trash
map gd cd ~/doc
map gp cd ~/pic
map gM cd ~/mus
map gw cd ~/doc/hemsida
map gm cd /mnt
map gu cd ~/doc/uppgifter
map gk cd ~/doc/kompendium
map gr cd /
map ge cd /etc
map gP cd /etc/pacman.d
map gU cd /usr
